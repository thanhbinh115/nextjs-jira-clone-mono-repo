{
  "name": "@full-stack/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:db-auth:create": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc db:create",
    "dev:db-auth:add-migration": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc migration:create --name create-user-table",
    "dev:db-auth:migrate": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc db:migrate",
    "dev:db-auth:add-seed": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc seed:generate --name seed-user-data",
    "dev:db-auth:seed": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc db:seed:all",
    "dev:db-auth:drop": "env-cmd -f ./env/local/app.env npx sequelize-cli --options-path ./db/.sequelizerc db:drop",
    "dev:db-auth:setup": "npm run dev:db-auth:drop && npm run dev:db-auth:create && npm run dev:db-auth:migrate && npm run dev:db-auth:seed",
    "prepare": "husky"
  },
  "private": true,
  "lint-staged": {
    "*": [
      "nx run api:lint",
      "npm run format",
      "git add ."
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/nest": "^21.4.1",
    "@nx/next": "^21.4.1",
    "@nx/node": "^21.4.1",
    "@nx/web": "21.4.1",
    "@nx/webpack": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "10.4.13",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^16.1.5",
    "nx": "21.4.1",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.6.3",
    "tailwindcss": "3.4.3",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "axios": "^1.6.0",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  }
}
